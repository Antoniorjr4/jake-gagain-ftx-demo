{
  "version": 2,
  "builds": [
    {
      "src": "api/process_ftx.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/process_ftx",
      "dest": "/api/process_ftx.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}